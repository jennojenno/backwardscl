<h1>Listing offers</h1>

<table>
  <tr>
    <th>Ownermake</th>
    <th>Ownermodel</th>
    <th>Owneryear</th>
    <th>Ownermileage</th>
    <th>Ownerdesc</th>
    <th>Ownercartype</th>
    <th>Offerprice</th>
    <th>Image1</th>
    <th>Seller</th>
    <th>Estvalue</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @offers.each do |offer| %>
  <tr>
    <td><%= offer.ownermake %></td>
    <td><%= offer.ownermodel %></td>
    <td><%= offer.owneryear %></td>
    <td><%= offer.ownermileage %></td>
    <td><%= offer.ownerdesc %></td>
    <td><%= offer.ownercartype %></td>
    <td><%= offer.offerprice %></td>
    <td><%= offer.image1 %></td>
    <td><%= offer.seller_id %></td>
    <td><%= offer.estvalue %></td>
    <td><%= link_to 'Show', offer %></td>
    <td><%= link_to 'Edit', edit_offer_path(offer) %></td>
    <td><%= link_to 'Destroy', offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Offer', new_offer_path %>
