<h1>Listing offers</h1>


<% @wanted_ad.offers.each do |a| %>
<div id="floatbox">

    <img src="<%= a.image1 %>">
   <div id="text"><BR><%= a.owneryear %> <%= a.ownermake %> <%= a.ownermodel %>
   
   <BR><%= a.ownermileage %> miles, Offer price $<%= a.offerprice %>
   
   <%= link_to 'Show', wanted_ad_offer_path(a.wanted_ad_id, a.id) %>
   <%= link_to 'Edit', edit_wanted_ad_offer_path(a.wanted_ad_id, a.id) %>


<% end %>

